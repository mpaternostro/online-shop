{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-underscore-dangle": ["error", { "allow": ["_id"]}]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
